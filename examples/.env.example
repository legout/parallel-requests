# Example Environment Variables
# Copy this file to .env and fill in your values

# Proxy Configuration
# Format: http://user:password@host:port or http://host:port
# Example: PROXY_URL=http://proxyuser:proxypass@proxy.example.com:8080
PROXY_URL=

# WebShare API Endpoint
# Get proxy list from webshare.io
# Example: WEBSHARE_URL=https://your-api-key:@proxy.webshare.io/api/v2/proxy/list?mode=direct&countries=all
WEBSHARE_URL=

# Optional: Multiple proxies (comma-separated)
# Example: PROXIES=192.168.1.1:8080,192.168.1.2:8080:admin:pass
PROXIES=

# Optional API Keys
# Examples may use these for demonstration
API_KEY=

# Optional: Custom User Agent
# Override default user agent rotation
# CUSTOM_USER_AGENT=MyCustomApp/1.0

# Optional: Rate Limiting Defaults
# PARALLEL_RATE_LIMIT=10
# PARALLEL_RATE_LIMIT_BURST=5

# Optional: Backend Selection
# PARALLEL_BACKEND=auto  # Options: auto, niquests, aiohttp, requests

# Optional: Concurrency
# PARALLEL_CONCURRENCY=20

# Optional: HTTP/2 Support
# PARALLEL_HTTP2=true

# Optional: Retry Settings
# PARALLEL_MAX_RETRIES=3

# Optional: Random User Agent
# PARALLEL_RANDOM_USER_AGENT=true

# Optional: Random Proxy
# PARALLEL_RANDOM_PROXY=false
